{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-BenTest"
		},
		"BensDataStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'BensDataStorage'"
		},
		"appdb_service_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'appdb_service'"
		},
		"appvm_server_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'appvm_server'"
		},
		"bens_pooldb_service_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'bens_pooldb_service'"
		},
		"bensstorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'bensstorage1'"
		},
		"bensstorage1_service_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'bensstorage1_service'"
		},
		"BensDataStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://bensdatastorage.dfs.core.windows.net/"
		},
		"appvm_server_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "D:\\nginx-1.24.0\\logs"
		},
		"appvm_server_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "adminBen"
		},
		"bensstorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://bensstorage1.dfs.core.windows.net/"
		},
		"bensstorage1_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://bensstorage1.dfs.core.windows.net/"
		},
		"StagingMetricTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/15315388-2a8e-4aa6-af96-d1780b61ac8a/resourceGroups/data-grp/providers/Microsoft.Storage/storageAccounts/bensdatastorage"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-BenTest_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"BensDataStorage_accountKey": {
						"value": "[parameters('BensDataStorage_accountKey')]"
					},
					"appdb_service_connectionString": {
						"value": "[parameters('appdb_service_connectionString')]"
					},
					"bens_pooldb_service_connectionString": {
						"value": "[parameters('bens_pooldb_service_connectionString')]"
					},
					"bensstorage1_accountKey": {
						"value": "[parameters('bensstorage1_accountKey')]"
					},
					"bensstorage1_service_accountKey": {
						"value": "[parameters('bensstorage1_service_accountKey')]"
					},
					"BensDataStorage_properties_typeProperties_url": {
						"value": "[parameters('BensDataStorage_properties_typeProperties_url')]"
					},
					"bensstorage1_properties_typeProperties_url": {
						"value": "[parameters('bensstorage1_properties_typeProperties_url')]"
					},
					"bensstorage1_service_properties_typeProperties_url": {
						"value": "[parameters('bensstorage1_service_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADF-BenTest_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"appvm_server_password": {
						"value": "[parameters('appvm_server_password')]"
					},
					"appvm_server_properties_typeProperties_host": {
						"value": "[parameters('appvm_server_properties_typeProperties_host')]"
					},
					"appvm_server_properties_typeProperties_userId": {
						"value": "[parameters('appvm_server_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-BenTest_ArmTemplate_0')]"
			]
		},
		{
			"name": "ADF-BenTest_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-BenTest_ArmTemplate_1')]"
			]
		},
		{
			"name": "ADF-BenTest_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"StagingMetricTrigger_properties_typeProperties_scope": {
						"value": "[parameters('StagingMetricTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-BenTest_ArmTemplate_2')]"
			]
		}
	]
}